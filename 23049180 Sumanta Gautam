   "id": "148acc5f-5874-491b-b94c-4d55f84283cd",
   "metadata": {},
   "source": [
    "<span style=\"font-size:30px\">HyperParameter tuning SVM</span>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "id": "90cdc947-c673-4e62-987d-766d805b0e79",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "0.9445048966267682"
      ]
     },
     "execution_count": 209,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sklearn.svm import LinearSVC\n",
    "\n",
    "# Define model\n",
    "svm = LinearSVC()\n",
    "\n",
    "# Parameter grid\n",
    "svm_params = {\n",
    "    'C': [0.01, 0.1, 1, 10],\n",
    "    'loss': ['hinge', 'squared_hinge']\n",
    "}\n",
    "\n",
    "# GridSearchCV\n",
    "svm_grid = GridSearchCV(\n",
    "    svm,\n",
    "    svm_params,\n",
    "    cv=5,\n",
    "    scoring='accuracy'\n",
    ")\n",
    "\n",
    "svm_grid.fit(X_train, y_train)\n",
    "\n",
    "# Best model\n",
    "best_svm = svm_grid.best_estimator_\n",
    "\n",
    "# Prediction\n",
    "svm_tuned_pred = best_svm.predict(X_test)\n",
    "\n",
    "# Accuracy\n",
    "svm_tuned_accuracy = accuracy_score(y_test, svm_tuned_pred)\n",
    "svm_tuned_accuracy\n"
   ]
  },
  {
