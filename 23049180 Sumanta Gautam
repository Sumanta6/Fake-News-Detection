   "source": [
    "from sklearn.model_selection import GridSearchCV\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "from sklearn.metrics import accuracy_score\n",
    "\n",
    "# Define model\n",
    "log_reg = LogisticRegression()\n",
    "\n",
    "# Define parameter grid\n",
    "log_reg_params = {\n",
    "    'C': [0.01, 0.1, 1, 10],\n",
    "    'solver': ['liblinear'],\n",
    "    'max_iter': [100, 200, 500]\n",
    "}\n",
    "\n",
    "# Apply GridSearchCV\n",
    "log_reg_grid = GridSearchCV(\n",
    "    log_reg,\n",
    "    log_reg_params,\n",
    "    cv=5,\n",
    "    scoring='accuracy'\n",
    ")\n",
    "\n",
    "log_reg_grid.fit(X_train, y_train)\n",
    "\n",
    "# Best model\n",
    "best_log_reg = log_reg_grid.best_estimator_\n",
    "\n",
    "# Prediction\n",
    "log_reg_tuned_pred = best_log_reg.predict(X_test)\n",
    "\n",
    "# Accuracy\n",
    "log_reg_tuned_accuracy = accuracy_score(y_test, log_reg_tuned_pred)\n",
    "log_reg_tuned_accuracy\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 198,
   "id": "25141369-e38c-4385-8385-56644c49ad20",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "lr_results = pd.DataFrame(log_reg_grid.cv_results_)\n",
    "\n",
    "plt.figure()\n",
    "plt.plot(lr_results['param_C'], lr_results['mean_test_score'])\n",
    "plt.xlabel(\"C Value\")\n",
    "plt.ylabel(\"Accuracy\")\n",
    "plt.title(\"Logistic Regression Hyperparameter Tuning\")\n",
    "plt.show()\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "3d155d20-7516-422f-8da6-c4e04387f1e3",
   "metadata": {},
   "source": [
    "<span style=\"font-size:30px\">ROC for Logistic Regression</span>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 199,
   "id": "2bf16c1c-4de4-41e6-ba2e-67915e866e31",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "np.float64(0.9860850620441413)"
      ]
     },
     "execution_count": 199,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from sklearn.metrics import roc_curve, roc_auc_score\n",
    "# Convert labels to binary (REAL=1, FAKE=0 if needed)\n",
    "y_test_binary = y_test.map({'FAKE': 0, 'REAL': 1})\n",
    "\n",
    "# Probability scores\n",
    "lr_probs = best_log_reg.predict_proba(X_test)[:, 1]\n",
    "\n",
    "# ROC values\n",
    "fpr_lr, tpr_lr, _ = roc_curve(y_test_binary, lr_probs)\n",
    "auc_lr = roc_auc_score(y_test_binary, lr_probs)\n",
    "\n",
    "auc_lr"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "c3183728-6665-42a2-ae3c-202f7db1dabf",
   "metadata": {},
